{
    "name": "hakaton",
    "version": "1.0.0",
    "description": "hakaton",
    "main": "main.js",
    "scripts": {
        "start": "webpack serve --open --config webpack.dev.js",
        "develop": "webpack serve --open --config webpack.dev.js",
        "build": "webpack --progress --config webpack.prod.js",
        "build-prod": "npm run build && npm run gzip",
        "build-dev": "webpack --progress --config webpack.dev.js",
        "gzip": "gzipper c --verbose --gzip-level 9 --exclude gz,jpg,jpeg,png,woff,woff2 ./public",
        "prettier:write": "prettier --config .prettierrc --write",
        "lint:js": "eslint --config .eslintrc.json --cache --color \"src/**/*.js\"",
        "lint:style": "stylelint \"src/**/*.scss\" --custom-syntax postcss-scss",
        "lint:style-fix": "stylelint --fix \"src/**/*.scss\" --custom-syntax postcss-scss"
    },
